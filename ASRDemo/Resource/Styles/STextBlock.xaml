<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ASRDemo.Resource.Styles">
    <Style x:Key="TextBlockStyle_ASRDemo_Normal" TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="Microsoft Yahei"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Foreground" Value="{StaticResource Secondary.Normal.Border}"/>
    </Style>
</ResourceDictionary>