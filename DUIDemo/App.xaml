                <ResourceDictionary Source="pack://application:,,,/DUIDemo;component/Resource/Styles/SComboBox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/DUIDemo;component/Resource/Styles/SToggleButton.xaml"/>
                <!--<ResourceDictionary Source="pack://application:,,,/DUIDemo;component/Resource/Styles/SConverter.xaml"/>-->
